    "url": "https://github.com/atorczynski/cryptosurge/issues"
  },
  "homepage": "https://github.com/atorczynski/cryptosurge#readme",
  "dependencies": {
    "crypto-news-api": "^2.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.0",
    "mongoose": "^5.10.2",
    "morgan": "^1.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
